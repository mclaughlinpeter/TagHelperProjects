        <title>Cities</title>
        <link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css"
            asp-fallback-href-include="/lib/bootstrap/dist/**/*.min.css"
            asp-fallback-href-exclude="**/*-reboot*,**/*-grid*"
